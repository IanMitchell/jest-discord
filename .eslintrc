{
  "extends": [
    "airbnb-base"
  ],
  "parser": "babel-eslint",
  "rules": {
    "class-methods-use-this": 0,
    "arrow-parens": [
      0,
      "as-needed",
      {
        "requireForBlockBody": false
      }
    ],
    "import/prefer-default-export": 0,
    "import/no-cycle": 0,
    "prefer-template": 0
  }
}
